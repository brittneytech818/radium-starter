{
  "name": "radium-starter",
  "version": "0.6.0",
  "description": "Base styles for Radium",
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/mvila/radium-starter"
  },
  "scripts": {
    "compile": "babel src --out-dir lib",
    "demo": "node demo",
    "release": "npm run lint && npm run compile && npm test && (git checkout --quiet master && test -z \"$(git status --porcelain)\" && npm version $npm_config_release_type && git push --follow-tags && npm publish) || echo \"An error occured (unclean repository?)\"",
    "release-patch": "npm run release --release-type=patch",
    "release-minor": "npm run release --release-type=minor",
    "release-major": "npm run release --release-type=major",
    "lint": "eslint .",
    "test": "echo \"Warning: no test specified\" && exit 0"
  },
  "dependencies": {
    "color": "^0.11.1",
    "event-emitter-mixin": "^0.3.1",
    "lodash": "^4.5.0",
    "radium-normalize": "^1.0.2",
    "reduce-css-calc": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.5.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "denodeify": "^1.2.1",
    "eslint": "^2.1.0",
    "eslint-config-next": "^0.1.0",
    "eslint-plugin-react": "^4.0.0-rc.1",
    "koa": "^2.0.0-alpha.3",
    "koa-convert": "^1.2.0",
    "koa-rewrite": "^1.1.1",
    "koa-static-cache": "^3.1.4",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-watch": "^0.3.5",
    "radium": "^0.16.6",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "rimraf": "^2.5.2",
    "uglify-js": "^2.6.1",
    "watchify": "^3.7.0"
  }
}
